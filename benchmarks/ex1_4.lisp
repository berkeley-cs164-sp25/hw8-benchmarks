(define (step x) (if (< 0 x) 1 0))
(define (xor x1 x2)
  (let ((h1 (- 0 x1)))
    (let ((h2 (- 0 x2)))
      (let ((s1 (step (+ x1 h2))))
        (let ((s2 (step (+ h1 x2))))
          (step (+ s1 s2))
            )))))
(do
(print (xor 0 0))
(newline)
(print (xor 0 1))
(newline)
(print (xor 1 0))
(newline)
(print (xor 1 1))
)